<KorOra>:
    id: krora
    GridLayout:
        size_hint: None, None
        size: self.parent.width, self.parent.height*0.15
        cols: 4
        AnchorLayout:
            anchor_x: "center"
            anchor_y: "center"
            size_hint: None, None
            size: self.parent.width*0.3, self.parent.height*0.8
            Button:
                id: a_t_k
                size_hint: None, None
                size: self.parent.width*0.8, self.parent.height*0.8
                text: "Age-Time"
                font_name: 'trebuc.ttf'
                font_size: 40
                color: [.75, .24, .09, 1]
                background_normal: 'button_normal.png'
                background_down: 'button_down.png'
                background_disabled_normal: 'button_normal.png'
                on_press: root.windowchangecancel()
                on_release: app.root.current = "korora"
        AnchorLayout:
            anchor_x: "center"
            anchor_y: "center"
            size_hint: None, None
            size: self.parent.width*0.3, self.parent.height*0.8
            Button:
                id: l_b_k
                size_hint: None, None
                size: self.parent.width*0.8, self.parent.height*0.8
                text: "Arrest-Type"
                font_name: 'trebuc.ttf'
                font_size: 40
                color: [.75, .24, .09, 1]
                background_normal: 'button_normal.png'
                background_down: 'button_down.png'
                background_disabled_normal: 'button_normal.png'
                on_press: root.windowchangecancel()
                on_release: app.root.current = "letbun"
        AnchorLayout:
            anchor_x: "center"
            anchor_y: "center"
            size_hint: None, None
            size: self.parent.width*0.3, self.parent.height*0.8
            Button:
                id: a_t_p_k
                size_hint: None, None
                size: self.parent.width*0.8, self.parent.height*0.8
                text: "Age-Time Pro"
                font_name: 'trebuc.ttf'
                font_size: 40
                color: [.75, .24, .09, 1]
                background_normal: 'button_normal.png'
                background_down: 'button_down.png'
                background_disabled_normal: 'button_normal.png'
                on_press: root.windowchangecancel()
                on_release: app.root.current = "folyt"
        AnchorLayout:
            anchor_x: "center"
            anchor_y: "center"
            size_hint: None, None
            size: self.parent.width*0.1, self.parent.height*0.8
            ToggleButton:
                size_hint: None, None
                size: self.parent.width*0.4, self.parent.height*0.6
                background_normal: 'hulp_normal.png'
                background_down: 'hulp_down.png'
                on_state: root.infopanel(self)
                
    Image:            
        size: self.texture_size
        id: img
        source: 'kor_ora/kor_ora0.png'
        
    GridLayout:
        size_hint: None, None
        size: self.parent.width, self.parent.height*0.2
        cols: 2
        GridLayout:
            cols: 3
            size_hint: None, None
            size: self.parent.width*0.1, self.parent.height
            AnchorLayout:
                anchor_x: "right"
                anchor_y: "center"
                size_hint: None, None
                size: self.parent.width*0.33, self.parent.height
                Button: # back
                    id: b_k
                    size_hint: None, None
                    size: self.parent.width, self.parent.height*0.25
                    background_normal: 'backward.png'
                    background_down: 'backward.png'
                    background_disabled_normal: 'backward.png'
                    on_press: root.back_speed()
            AnchorLayout:
                size_hint: None, None
                size: self.parent.width*0.34, self.parent.height
                anchor_x: "center"
                anchor_y: "center"
                ToggleButton: #startstop
                    id: toggle_ko
                    size_hint: None, None
                    size: self.parent.width, self.parent.height*0.25
                    #id: butt
                    text: ""
                    background_normal: 'start.png'
                    background_down: 'stop.png'
                    background_disabled_normal: 'start.png'
                    on_state: root.start_stop(self)
            AnchorLayout:
                size_hint: None, None
                size: self.parent.width*0.33, self.parent.height
                anchor_x: "left"
                anchor_y: "center"
                Button: #forward
                    id: f_k
                    size_hint: None, None
                    size: self.parent.width, self.parent.height*0.25
                    background_normal: 'forward.png'
                    background_down: 'forward.png'
                    background_disabled_normal: 'forward.png'
                    on_press: root.forw_speed()
        Slider:
            size_hint: None, None
            size: self.parent.width*0.9, self.parent.height
            id: slid
            min: 0
            max: 71
            cursor_image: "./knob.png"
            cursor_disabled_image: "./knob.png"
            on_value: root.on_slider_value(self) 
        
        
