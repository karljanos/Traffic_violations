#:import NoTransition kivy.uix.screenmanager.NoTransition
WindowManager:
    transition: NoTransition()
    KorOraWindow:
    LetBunWindow:
    CanvasFolytWindow:
    
<KorOraWindow>:
    name: "korora"
    #on_enter: self.reset_but()
    KorOra:

<LetBunWindow>:
    name: "letbun"           
    on_enter: self.reset_but()  
    LetBun:
    
<CanvasFolytWindow>:
    name: "folyt"
    #on_enter: self.reset_but()
